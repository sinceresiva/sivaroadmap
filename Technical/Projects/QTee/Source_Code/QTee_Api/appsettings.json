{
  "spring": {
    "application": {
      "name": "QTeeAPI"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "label": "development",        
        "searchPaths": "{label}/{application}",
        "validate_certificates": false       
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}