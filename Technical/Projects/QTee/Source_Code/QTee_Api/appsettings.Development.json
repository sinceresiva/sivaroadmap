{
  "spring": {
    "application": {
      "name": "QTeeAPI"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certificates": false,
        "git": {
            "uri": "https://github.com/sinceresiva/config-repo.git",
            "searchPaths": "{label}/{application}"
        }
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}